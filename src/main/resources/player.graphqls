type Player {
    id: ID!,
    name: String!,
    surname: String!,
    position: String!,
}

input PlayerDto {
    name: String!,
    surname: String!,
    position: String!
}

type Query {
    getPlayers:[Player]
    getPlayers(position: String):[Player]
    getById(id: ID):Player
}

type Mutation {
    createPlayer(player: PlayerDto):Player
}